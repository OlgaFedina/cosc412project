<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- insert your AIML categories here -->
	
	<category>
    <pattern>I AM BORED</pattern>
    <template>Aw, I'm sorry to hear that! Maybe you can play a game.</template>
  </category>
  
  <category>
    <pattern>I*M BORED</pattern>
    <template>Sit outside and eat a banana.</template>
  </category>
  
  <category>
    <pattern>I MISS MY FRIENDS</pattern>
    <template>I'm sure they miss you too! Maybe call one of them?</template>
  </category>
  
  <category>
    <pattern>WHAT*S YOUR NAME*</pattern>
    <template>My name is Kenny! But you can call me Ken.</template>
  </category>
  
  <category>
    <pattern>* I LOVE YOU *</pattern>
    <template>You barely know me.</template>
  </category>
</aiml>